.question-ranking-rank-wrapper {
  max-width: 100%;
  margin: 0px 0px 5px;
}

.question-ranking-rank {
  position: relative;
  vertical-align: middle;
  float: none;
  width: auto;
  height: auto;
  min-height: 25px;
  line-height: 25px;
  z-index: 0;
  background-clip: padding-box;
  border-width: 1px;
  border-style: solid;
  border-color: rgb(204, 204, 204);
  border-image: initial;
  padding: 5px 5px 5px 15px;
  overflow: auto;
  border-radius: 3px;
}

.question-ranking-table-wrapper {
  display: table;
  width: 100%;
}

.question-ranking-table-row-wrapper {
  display: table-row;
}

.question-ranking-bullet-cell {
  display: table-cell;
  width: 42px;
}

.question-ranking-bullet {
  background-size: 9px 13px;
}

.question-ranking-select-cell {
  display: table-cell;
  width: 62px;
}

.question-ranking-select-cell, select.no-touch {
  background-image: url(/assets/responseweb/smlib.surveytemplates/4.1.87/assets/arrow_dropdown_with_padding.png);
  background-color: white;
  background-repeat: no-repeat;
  background-position: right center;
  padding-left: 5px;
  min-height: 24px;
  -webkit-appearance: none;
  padding-right: 20px;
  border-radius: 5px;
  font-size: 16px;
  max-width: 100%;
  box-sizing: border-box;
  background: padding-box rgb(255, 255, 255);
  border-width: 1px;
  border-style: solid;
  border-color: rgb(153, 153, 153);
  border-image: initial;
  padding: 6px;
  transition: all 0.1s linear 0s;
  margin-right: 5px;
  vertical-align: middle;
  display: inline-block;
  width: 60px;
  overflow: auto;
  font-family: "Helvetica Neue", Arial, sans-serif;
  color: rgb(51, 51, 51);
  margin: 0px;
  overflow: visible !important;
  align-items: center;
  white-space: pre;
  -webkit-rtl-ordering: logical;
  cursor: default;
  text-rendering: auto;
  letter-spacing: normal;
  word-spacing: normal;
  text-transform: none;
  text-indent: 0px;
  text-shadow: none;
  text-align: start;
  font: 400 11px system-ui;
  -webkit-writing-mode: horizontal-tb !important;
}

.survey-page .question-body select {
  font-size: 16px;
}

.question-ranking-answer-cell {
  width: auto;
  display: table-cell;
}

.question-body-font-theme {
  color: rgb(51, 62, 72);
  font-size: 16px;
  font-weight: 300;
  font-style: normal;
  text-decoration: none;
}

.answer-label-rank {
  display: inline-block;
  vertical-align: middle;
  padding-top: 3px;
  cursor: move;
  width: 100%;
  margin-left: 2px;
}

.answer-label {
  word-wrap: break-word;
  white-space: normal;
  display: block;
  padding: 0px 0px 5px;
}
